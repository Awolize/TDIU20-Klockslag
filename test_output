TEST_CASE("const to_string() ")
{
    Time const t{14,12,12};
    Time const t1{03,03,03};


    INFO("Time const t{14,12,12};\nTime const t1{03,03,03};")
    CHECK(t.to_string() == "14:12:12");
    CHECK(t.to_string(false) == "14:12:12");
    CHECK(t.to_string(true) == "02:12:12 pm");
    CHECK(t1.to_string() == "03:03:03");
    CHECK(t1.to_string(false) == "03:03:03");
    CHECK(t1.to_string(true) == "03:03:03 am");
}


