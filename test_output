TEST_CASE("string operator")
{
    Time t{"14:12:12"};
    Time t1{"03:03:03"};
    Time const t2{"14:12:12"};
    Time const t3{"03:03:03"};

    CHECK(static_cast<std::string>(t) == "14:12:12");
    CHECK(std::string(t1) == "03:03:03");
    CHECK(static_cast<std::string>(t2) == "14:12:12");
    CHECK(std::string(t3) == "03:03:03");
}


